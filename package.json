  "author": "nuvopoint",
  "license": "MIT",
  "version": "0.0.5",
  "homepage": "https://github.com/Eonfire/cordova-plugin-acs/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eonfire/cordova-plugin-acs"
